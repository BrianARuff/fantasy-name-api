    "noImplicitAny": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "outDir": "./dist",
    "sourceMap": true,
    "moduleResolution": "Node"
  },
  "include": ["src/**/*", "./src/*"],
  "exclude": ["node_modules", "**/*.spec.ts"]
}